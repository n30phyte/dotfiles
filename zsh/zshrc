source ~/.antigen.zsh

# Antigen setup start
antigen use oh-my-zsh

antigen bundle git
antigen bundle dnf
antigen bundle pip
antigen bundle python
antigen bundle sudo
antigen bundle jsontools

antigen bundle zsh-users/zsh-autosuggestions
antigen bundle unixorn/autoupdate-antigen.zshplugin

antigen theme romkatv/powerlevel10k

antigen apply
# Antigen setup end

# Shell setup start
CASE_SENSITIVE="true"
unsetopt histverify
setopt +o nomatch
# Shell setup end

# Source your custom files
if [ -f ~/.zshprofile ]; then
    source ~/.zshprofile
else
    touch ~/.zshprofile
fi

[[ ! -f ~/.p10k.zsh ]] || source ~/.p10k.zsh
